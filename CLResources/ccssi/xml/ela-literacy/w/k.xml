      <LearningStandardItemRefId RelationshipType="childOf">FF4015202DCF44e1BABFCCFF255C5963</LearningStandardItemRefId>
    </RelatedLearningStandardItems>
  </LearningStandardItem>
  <LearningStandardItem xml:lang="en" RefID="F411CC33001F4c18AFCB739FBCE74A4E">
    <RefURI>http://corestandards.org/ELA-Literacy/W/K/2/</RefURI>
    <StandardHierarchyLevel>
      <number>6</number>
      <description>Standard</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.ELA-Literacy.W.K.2</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Use a combination of drawing, dictating, and writing to compose informative/explanatory texts in which they name what they are writing about and supply some information about the topic.</Statement>
    </Statements>
