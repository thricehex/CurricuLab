    <StandardHierarchyLevel>
      <number>6</number>
      <description>Standard</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.ELA-Literacy.RI.11-12.7</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Integrate and evaluate multiple sources of information presented in different media or formats (e.g., visually, quantitatively) as well as in words in order to address a question or solve a problem.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>11-12</GradeLevel>
    </GradeLevels>
