  <LearningStandardItem xml:lang="en" RefID="A066C50085664081AB2949592CEC6A29">
    <RefURI>http://corestandards.org/ELA-Literacy/RI/11-12/2/</RefURI>
    <StandardHierarchyLevel>
      <number>6</number>
      <description>Standard</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.ELA-Literacy.RI.11-12.2</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Determine two or more central ideas of a text and analyze their development over the course of the text, including how they interact and build on one another to provide a complex analysis; provide an objective summary of the text.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>11-12</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>0FAC40650F514a459951A657F942B845</LearningStandardDocumentRefId>
