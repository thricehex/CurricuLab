  <LearningStandardItem xml:lang="en" RefID="C1A47EC07F9F49C481DE52A8235E27B6">
    <RefURI>http://corestandards.org/Math/Content/6/EE/A/2/a/</RefURI>
    <StandardHierarchyLevel>
      <number>8</number>
      <description>Component</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.Math.Content.6.EE.A.2a</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Write expressions that record operations with numbers and with letters standing for numbers. &lt;i&gt;For example, express the calculation &amp;ldquo;Subtract y from 5&amp;rdquo; as 5 &amp;ndash; y&lt;/i&gt;.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>06</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>B62C1C106873438AA0126760075A65A3</LearningStandardDocumentRefId>
