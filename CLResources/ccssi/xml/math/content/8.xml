    <StatementCodes>
      <StatementCode>CCSS.Math.Content.8.EE.C.7</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Solve linear equations in one variable.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>08</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>B62C1C106873438AA0126760075A65A3</LearningStandardDocumentRefId>
