  <LearningStandardItem xml:lang="en" RefID="C5B0127A97064227B311F52DC8A657C9">
    <RefURI>http://corestandards.org/Math/Content/8/G/A/1/a/</RefURI>
    <StandardHierarchyLevel>
      <number>8</number>
      <description>Component</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.Math.Content.8.G.A.1a</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Lines are taken to lines, and line segments to line segments of the same length.</Statement>
    </Statements>
    <GradeLevels>
