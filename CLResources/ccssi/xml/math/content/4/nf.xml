  <LearningStandardItem xml:lang="en" RefID="D7B0A4C892974E05B3789666B34F8C50">
    <RefURI>http://corestandards.org/Math/Content/4/NF/B/4/</RefURI>
    <StandardHierarchyLevel>
      <number>7</number>
      <description>Standard</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.Math.Content.4.NF.B.4</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Apply and extend previous understandings of multiplication to multiply a fraction by a whole number.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>04</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>B62C1C106873438AA0126760075A65A3</LearningStandardDocumentRefId>
