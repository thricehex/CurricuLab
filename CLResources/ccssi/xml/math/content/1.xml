      <LearningStandardItemRefId RelationshipType="childOf">FDFBABAA6BFA4FBDACA10CA3898A2D6E</LearningStandardItemRefId>
    </RelatedLearningStandardItems>
  </LearningStandardItem>
  <LearningStandardItem xml:lang="en" RefID="2688E9D1A3FA4B689A3D9E41A1071C0E">
    <RefURI>http://corestandards.org/Math/Content/1/OA/C/5/</RefURI>
    <StandardHierarchyLevel>
      <number>7</number>
      <description>Standard</description>
    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.Math.Content.1.OA.C.5</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Relate counting to addition and subtraction (e.g., by counting on 2 to add 2).</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>01</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>B62C1C106873438AA0126760075A65A3</LearningStandardDocumentRefId>
