    </StandardHierarchyLevel>
    <StatementCodes>
      <StatementCode>CCSS.Math.Content.4.NBT.A.3</StatementCode>
    </StatementCodes>
    <Statements>
      <Statement>Use place value understanding to round multi-digit whole numbers to any place.</Statement>
    </Statements>
    <GradeLevels>
      <GradeLevel>04</GradeLevel>
    </GradeLevels>
    <LearningStandardDocumentRefId>B62C1C106873438AA0126760075A65A3</LearningStandardDocumentRefId>
